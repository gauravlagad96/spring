<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

	<bean class="com.autowired.Address" id="add">
		<property name="street" value="kolgaon" />
		<property name="city" value="shrigonda" />
	</bean>

	<bean class="com.autowired.Address" id="add2">
		<property name="street" value="kolgaon2" />
		<property name="city" value="shrigonda2" />
	</bean>

	<!-- <bean class="com.autowired.Employee" id="emp" autowire="byName"/> class 
		ref and bean id are same -->

	<bean class="com.autowired.Employee" id="emp" autowire="byType" />
	<!-- <property name="address" ref="add"/> ctrl+shift+c -->





</beans>